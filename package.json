{
  "name": "standalone-crc",
  "version": "0.0.0",
  "description": "Run crc locally!",
  "private": true,
  "scripts": {
    "start:auth": "sudo ./packages/insights-auth/start_keycloak.sh",
    "start:api": "yarn workspace insights-api start",
    "start:backend": "yarn start:auth && yarn start:api",
    "start:frontend": "yarn workspace insights-starter start",
    "build:chrome": "yarn workspace insights-chrome build",
    "uninstall": "find . -name node_modules | xargs rm -rf"
  },
  "author": "Red Hat",
  "license": "MIT",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  }
}
